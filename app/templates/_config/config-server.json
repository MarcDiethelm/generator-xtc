// Never commit authentication data to a public repository!
{
	//  Default html > head > title is constructed from this
	"siteName": "${siteName}"
	//,"allowAuthBypassForIpRanges": true
	//,"annotateModules": {
	//	"production": true
	//}
	//,"enableModuleTesting": true
	//  To construct links to views, modules and layouts in your repository's web UI ("read-only" https .git URI)
	,"repository": "${repository.uri}"
	,"repositoryBranch": "${repository.branch}"
	<% if (!needBuild) { %>
	// URI prefix for static assets, e.g. '/static', empty string for none. Also used inside generated css files as base path.
	,"staticBaseUri"    : ""
	<% } %>

}